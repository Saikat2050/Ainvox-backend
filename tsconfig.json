{
    "compilerOptions": {
        // "target": "es6",
        // "module": "commonjs",
        "target": "ES2020",
        "module": "nodenext",
        "rootDir": "src",
        "outDir": "dist",
        "moduleResolution": "NodeNext",
        "strict": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "allowJs": false
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }